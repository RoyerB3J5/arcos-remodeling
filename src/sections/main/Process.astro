---
import ProcessContent from '../ProcessContent.astro';

type Translations = typeof import('../../i18n/es').default;
type ProcessContent = Translations['main']['process'];
interface Props {
  content: ProcessContent;
}
const { content } = Astro.props as Props;
---

<section
  class="w-full max-w-screen md:max-w-7xl flex flex-col justify-center items-center px-5 xl:px-0 gap-10 md:gap-14 pt-14 lg:pt-30"
>
  <ProcessContent content={content} />
</section>
